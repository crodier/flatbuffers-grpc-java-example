namespace com.example.fbgrpc.flatbuffers;

rpc_service ExampleServer {
    DoWorkBidi(Request): Response (streaming: "bidi");
    DoWork(Request): Response (streaming: "none");
}

table Request{
    // work: string;
    time:long;
    id:long;
}

table Response{
    // result: string;
    time:long;
    id:long;
}